<!-- header -->
<%-include partials/header %>

<h1>Home</h1>
<p> <%= homeContent %></p>

<% postTextEJS.forEach(function(element){ %>
    <h1><%=element.title%></h1>
    <%if(element.body.length >100){ %>
        <p><%=element.body.substring(0,100)%> ... <a href = '/posts/<%=element.title %>'>Read More</a></p>
    <%}else{%>
        <p><%=element.body %></p>
    <%}%>
  
<% }) %>

<!-- footer -->
<%-include partials/footer %>









